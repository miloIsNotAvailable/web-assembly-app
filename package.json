{
  "name": "fapp",
  "private": true,
  "version": "0.0.0",
  "module": "esnext",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "glob": "^8.0.3",
    "node-fetch": "^3.2.10",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "three": "^0.145.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/three": "^0.144.0",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
